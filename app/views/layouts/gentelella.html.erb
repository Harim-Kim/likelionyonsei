<!DOCTYPE html>
<html>
<head>
  <title>연대멋사</title>

  <%= stylesheet_link_tag    'gentelella', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
	<%= render "partials/google" %>
</head>

<body class="nav-md">
<div class="container body">
  <% flash.each do |key, value| %>
      <div align="center" id="flash" class="alert alert-<%= key %>"><%= value %></div>
  <% end %>



<%= yield %>

  
  <%= javascript_include_tag 'footermanifest', 'data-turbolinks-track' => true %>
  <script src="https://use.fontawesome.com/8d2db7e54e.js"></script>

</body>
</html>
