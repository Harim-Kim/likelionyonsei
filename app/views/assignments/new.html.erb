
<div class="container">
  <h1><%=@assign.title%></h1>
  <h4><%=@assign.content%></h4>
  <form action="/assignments/create" method="post" enctype="multipart/form-data">
    제목 <input type="text" name="title" class="form-control"><br>
    c9_url <input type="text" name="c9_url" class="form-control"><br>
    내용 <textarea rows="4" class="form-control" type="text" name="content"></textarea><br>
     <input type="file" name="pic" accept="image/*">
     <input name="an" type="hidden" value="<%=@num%>">
    <input type="submit" value="Submit">
  </form>
</div>
